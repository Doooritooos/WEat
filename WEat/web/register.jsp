    <body class="homepage">

        <!-- Header Wrapper -->
        <div id="header-wrapper">

            <!-- Header -->
            <div id="header" class="container">

                <!-- Logo -->
                <h1 id="logo"><a href="index.html">WEat</a></h1>


            </div>



        </div>
    </div>

    <!-- Footer Wrapper -->
    <div id="footer-wrapper">

        <!-- Footer -->
        <div id="footer" class="container">
            <header class="major">
                <h2>WEat</h2>
                <span>Please Register</span>
            </header>
            <div class="row">
                <section class="12u">
                    <form method="post" action="RegisterServlet">
                        <div class="row half">
                            <div class="6u">
                                <input name="email" placeholder="Email" type="email" class="text" />
                            </div>
                            <div class="6u">
                                <input name="password" placeholder="Password" type="password" class="text" />
                            </div>
                        </div>
                        <div class="row half">
                            <div class="12u">
                                <input name="username" placeholder="Username" type="text" class="text" />
                            </div>
                        </div>
                        <div class="row half">
                            <div class="3u">
                                &nbsp
                            </div>
                            <div class="3u">
                                <ul class="actions">
                                    <li><input type="submit" class="button" value="Register"></li>


                                </ul>
                            </div>

                            <div class="2u">
                                &nbsp
                            </div>
                            <div class="4u">
                                <ul class="actions">
                                    <li><a href="index.jsp"><input type="button" class="button" value="Cancel"></a></li>

                                </ul>
                            </div>
                        </div>
                    </form>
                </section>

            </div>
